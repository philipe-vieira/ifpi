package prova03;

import javax.swing.JOptionPane;

public class Questao03 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Calculadora use = new Calculadora();
		use.inserir();
		JOptionPane.showMessageDialog(null, "Soma = "+use.Somar());
		JOptionPane.showMessageDialog(null, "Subtração = "+use.Subtrair());
		JOptionPane.showMessageDialog(null, "Multiplicação = "+use.Multiplicar());
	    JOptionPane.showMessageDialog(null, "Divisão = "+use.Dividir());

	}

}
